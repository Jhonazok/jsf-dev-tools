{
  "manifest_version": 2,
  "name": "JSF Development Tools",
  "description": "JSF Development Tools",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
      "tabs", 
      "http://*/*", 
      "activeTab"
  ],
  "browser_action": {
    "default_title": "JSF Development Tools",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    }
  },
  "web_accessible_resources": [ 
    "inject.js" 
  ]
}